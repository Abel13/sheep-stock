{
  "name": "sheep-stock",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "ios:device": "npx expo run ios --device",
    "android:device": "npx expo run android --device",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build:ios": "npx eas build --platform ios --profile preview",
    "build:android": "npx eas build --platform android --profile preview"
  },
  "jest": {
    "maxWorkers": 2,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|tamagui)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.{ts,tsx}"
    ],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/drawer": "^7.3.9",
    "@react-navigation/native": "^7.0.14",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.50.0",
    "@tamagui/animations-moti": "^1.126.13",
    "@tamagui/babel-plugin": "^1.126.13",
    "@tamagui/config": "^1.126.13",
    "@tamagui/lucide-icons": "^1.126.13",
    "@tamagui/toast": "^1.126.13",
    "@tanstack/react-query": "^5.59.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "date-fns": "^4.1.0",
    "expo": "^54.0.29",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.10",
    "expo-document-picker": "~14.0.8",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.10",
    "expo-print": "~15.0.8",
    "expo-router": "~6.0.19",
    "expo-sharing": "~14.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "lottie-react-native": "~7.3.1",
    "nativewind": "4.0.36",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.81.5",
    "react-native-date-picker": "^5.0.7",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.4.13",
    "tamagui": "^1.126.13",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@eslint/js": "^9.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "ajv": "^7.2.4",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "jest-canvas-mock": "^2.5.2",
    "jest-expo": "~54.0.16",
    "jscodeshift": "^0.15.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.3.1",
    "supabase": "^2.24.3",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.17.0"
  },
  "private": true,
  "resolutions": {
    "ajv": "^6.12.6"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "@tamagui/config",
          "@tamagui/toast",
          "aws-sdk",
          "tailwindcss",
          "tamagui"
        ]
      }
    }
  }
}
