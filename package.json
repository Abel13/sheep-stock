{
  "name": "sheep-stock",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "ios:device": "npx expo run ios --device",
    "android:device": "npx expo run android --device",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "jest": {
    "maxWorkers": 2,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|tamagui)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testMatch": [
      "<rootDir>/tests/**/*.test.{ts,tsx}"
    ],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/native": "^7.0.14",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.49.6",
    "@tamagui/animations-moti": "^1.126.13",
    "@tamagui/babel-plugin": "^1.126.13",
    "@tamagui/config": "^1.126.13",
    "@tamagui/lucide-icons": "^1.126.13",
    "@tamagui/toast": "^1.126.13",
    "@tanstack/react-query": "^5.59.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.12.2",
    "expo": "^53.0.9",
    "expo-camera": "~16.1.6",
    "expo-constants": "~17.1.6",
    "expo-document-picker": "~13.1.5",
    "expo-font": "~13.3.1",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.5",
    "expo-print": "~14.1.4",
    "expo-router": "~5.0.7",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.7",
    "expo-updates": "~0.28.13",
    "expo-web-browser": "~14.1.6",
    "nativewind": "4.0.36",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.79.2",
    "react-native-date-picker": "^5.0.7",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "tailwindcss": "^3.4.13",
    "tamagui": "^1.126.13",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@eslint/js": "^9.16.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "ajv": "^7.2.4",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.2.1",
    "jest-canvas-mock": "^2.5.2",
    "jest-expo": "~53.0.5",
    "jscodeshift": "^0.15.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.3.1",
    "supabase": "^1.200.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.17.0"
  },
  "private": true,
  "resolutions": {
    "ajv": "^6.12.6"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false,
        "exclude": [
          "@tamagui/config",
          "@tamagui/toast",
          "aws-sdk",
          "tailwindcss",
          "tamagui"
        ]
      }
    }
  }
}
